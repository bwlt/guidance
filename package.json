{
  "name": "guidance",
  "version": "0.3.0",
  "description": "A Rails like routing system for Express.js",
  "keywords": [
    "routing",
    "express",
    "expressjs",
    "rails"
  ],
  "main": "./lib",
  "scripts": {
    "coverage": "istanbul cover _mocha -- ./test/ --recursive",
