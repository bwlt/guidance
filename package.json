    "test": "mocha",
    "test:watch": "npm test -- --watch"
  },
  "author": {
    "name": "Walter Barbagallo",
    "email": "brb.walter@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/turbometalskater/guidance.git"
  },
  "devDependencies": {
    "async": "^2.0.0-rc.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "express": "^4.13.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "depd": "^1.1.0",
    "lodash": "^4.6.1",
    "methods": "^1.1.2",
    "pluralize": "^1.2.1",
    "require-dir": "^0.3.0"
